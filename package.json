{
  "name": "kstor",
  "version": "1.0.6",
  "description": "Simple key value storage.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "node ./build clean",
    "copy": "node ./build copy",
    "compile": "node ./build compile",
    "bump": "node ./build bump",
    "build": "node ./build build",
    "commit": "node ./build commit",
    "docs": "node ./build docs",
    "release": "node ./build release",
    "test": "node ./build test"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blujedis/kstor.git"
  },
  "build": {
    "clean": [
      "./dist"
    ],
    "copy": []
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "./build/spec"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [],
  "author": "Blu Jedi <blujedicorp@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blujedis/kstor/issues"
  },
  "homepage": "https://github.com/blujedis/kstor#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/graceful-fs": "^4.1.2",
    "@types/make-dir": "^1.0.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.28",
    "@types/write-file-atomic": "^2.1.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "stiks": "^1.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "chek": "1.1.7",
    "graceful-fs": "^4.1.11",
    "make-dir": "^1.3.0",
    "scmp": "^2.0.0",
    "simple-encryptor": "^1.3.0",
    "write-file-atomic": "^2.3.0"
  }
}